	pod 'Moya-ObjectMapper/RxSwift'
	pod 'RxSwift'
        pod 'RxOptional'
	pod 'RxCocoa'
	pod 'Wrap'
	pod 'Moscapsule', :git => 'https://github.com/flightonary/Moscapsule.git'
end

