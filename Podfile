	pod 'Moya', '8.0.0-beta.5'
	pod 'Moya-ObjectMapper/RxSwift'
	pod 'Moya/RxSwift'
	pod 'RxSwift'
	pod 'RxCocoa'
	pod 'RxOptional'
	pod 'Wrap'
